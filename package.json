{
	"publisher": "uttntt",
	"name": "dimloglines",
	"displayName": "DimLogLines",
	"description": "dim log lines",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"extensionPack": [ "uttntt.dimloglines"],
	"activationEvents": [
		"onCommand:dimloglines.dim",
		"onCommand:undimloglines.undim"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "dimloglines.dim",
				"title": "dim log lines"
			},
			{
				"command": "dimloglines.undim",
				"title": "undim log lines"
			}
		],
		"configuration": {
      "properties": {
        "dimloglines.opacity": {
          "default": 0.2,
          "type": "number",
          "description": "value between 0 and 1 used for the opacity for the dimmed log lines"
				},
				"dimloglines.regExps": {
					"type" : "array",
					"default": [
						"\\w*LOG\\s*\\([\\s\\S]*?\\)?\\s*?;",
						"\\w*[lL]og.*?\\.(Write|assert|debug|warning|info|information|report|success|error|fail|exception)\\s*\\(([\\s\\S]*?)\\)?\\s*?;"
					],
					"description": "regular expressions to identify log line"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
